# mysql
DB_PORT="3306"

# backend
BACKEND_PORT="18080"
STUDIO_PORT="5555"

# Dockerfile <mysql>
MYSQL_DATABASE: 'anidge'
MYSQL_ROOT_PASSWORD: ''
MYSQL_USER: 'anidge'
MYSQL_PASSWORD: ''
TZ="Asia/Tokyo"

# Dockerfile <backend>
DATABASE_URL="mysql://anidge:****@db:3306/anidge?connect_timeout=30"

# JWT
JWT_SECRET=